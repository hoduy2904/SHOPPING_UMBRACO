@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "_Layout.cshtml";
}
@{Html.RenderPartial("banners"); }
<div class="container wrapper">

    @foreach (var item in Model.Children)
    {
        switch (item.ContentType.Alias)
        {
            case "categoryTemplate":
                {
                    Html.RenderPartial("Categories");
                    break;
                }
            case "bannerThreeTemplate":
                {
                    Html.RenderPartial("BannerThree");
                    break;
                }
            case "bannerTwoTemplate":
                {
                    Html.RenderPartial("BannerTwo");
                    break;
                }
            case "productsTemplate":
                {
                    Html.RenderPartial("Products");
                    break;
                }
        }
    }
   
</div>
